    "babel-plugin-import-noop": "1.0.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-preset-env": "1.6.0",
    "clean-webpack-plugin": "0.1.16",
    "css-loader": "0.28.4",
    "eslint": "4.2.0",
    "eslint-config-angular": "0.5.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-angular": "3.0.0",
