    "sass-resources-loader": "1.2.0",
    "style-loader": "0.16.1",
    "url-loader": "0.5.8",
    "webpack": "2.3.2",
    "webpack-dev-server": "2.4.2",
    "webpack-merge": "4.1.0"
  },
  "dependencies": {
    "angular": "1.6.4",
    "angular-animate": "1.6.4",
    "angular-aria": "1.6.4",
    "angular-material": "1.1.3",
    "angular-mocks": "1.6.4",
